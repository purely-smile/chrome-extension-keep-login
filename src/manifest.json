{
  "name": "keep login",
  "version": "0.0.1",
  "background": {
      "scripts": ["background.js"],
      "persistent": true
    },
  "manifest_version": 2,
  "permissions": [
      "webRequest",
      "*://*.aibeike.com/*",
      "activeTab",
      "cookies",
      "storage",
      "notifications",
      "tabs"
  ],
  "browser_action": {
      "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  }
}
